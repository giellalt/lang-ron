+N+Fem+Gen+Pl+Indef:%>e # ;
+N+Fem+Gen+Pl+Def:%>ele # ;
+N+Fem+Voc+Sg+Indef:ă # ;
+N+Fem+Voc+Sg+Def:%>ă # ;
+N+Fem+Voc+Pl+Def:%>elor # ;

LEXICON f_uri  !  pl -uri
+N+Fem+Nom+Sg+Indef: # ;
+N+Fem+Nom+Sg+Def:%>a # ;
+N+Fem+Nom+Pl+Indef:%>uri # ;

+N+Fem+Nom+Pl+Def:%>urile # ;
