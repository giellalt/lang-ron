! ======================
!! !!!Sublexica for Noun
! ======================


! Sone 2a Deklinasjonsklassane
! ============================

! m
! -

LEXICON m ! default pl -i
+N+Msc+Nom+Sg+Indef: # ;
+N+Msc+Nom+Sg+Def:%>ul # ;
+N+Msc+Nom+Pl+Indef:%>i # ;
+N+Msc+Nom+Pl+Def:%>ii # ;
+N+Msc+Gen+Sg+Indef: # ;
+N+Msc+Gen+Sg+Def:%>ului # ;
+N+Msc+Gen+Pl+Indef:%>i # ;
+N+Msc+Gen+Pl+Def:%>ilor # ;
+N+Msc+Voc+Sg+Indef:e # ;
+N+Msc+Voc+Sg+Def:%>ule # ;
+N+Msc+Voc+Pl+Def:%>ilor # ;


! f
! -

LEXICON f  ! default pl -e
+N+Fem+Nom+Sg+Indef: # ;
+N+Fem+Nom+Sg+Def:%>a # ;
+N+Fem+Nom+Pl+Indef:%>e # ;

+N+Fem+Nom+Pl+Def:%>ele # ;
+N+Fem+Gen+Sg+Indef:e # ;
+N+Fem+Gen+Sg+Def:%>ei # ;
+N+Fem+Gen+Pl+Indef:%>e # ;
+N+Fem+Gen+Pl+Def:%>ele # ;
+N+Fem+Voc+Sg+Indef:ă # ;
+N+Fem+Voc+Sg+Def:%>ă # ;
+N+Fem+Voc+Pl+Def:%>elor # ;

LEXICON f_i  !  pl -uri
+N+Fem+Nom+Sg+Indef: # ;
+N+Fem+Nom+Sg+Def:%>a # ;
+N+Fem+Nom+Pl+Indef:%>i # ;

+N+Fem+Nom+Pl+Def:%>ele # ;
+N+Fem+Gen+Sg+Indef:e # ;
